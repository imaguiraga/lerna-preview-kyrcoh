const { terminal } = require('@imaguiraga/topology-dsl-core');
const mapArray = require('./{{ provider }}-map');

{% for item in items %}
/**
 * Create a '{{ item.dsl }}' dsl tree.
 * @param {object} elt - The element.
 * @return {object} dsl object.
 */
function {{ item.dsl }}(elt) {
  return {{ item.tagName }}(elt)._subType_('{{ item.subType }}')._set_('style', mapArray.get('{{ item.dsl }}'));
}
module.exports.{{ item.dsl }} = {{ item.dsl }};

{% endfor %}
